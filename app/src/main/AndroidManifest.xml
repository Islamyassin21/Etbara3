<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.islam.islam.etbara3">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-sdk
        android:maxSdkVersion="15"
        android:targetSdkVersion="25" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/arzak4"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name="com.islam.islam.etbara3.MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.islam.islam.etbara3.DetailsActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.islam.islam.etbara3.MainActivity" />
        </activity>

        <receiver android:name="com.islam.islam.etbara3.SMS">
            <intent-filter>
                <action android:name="in.wptrafficanalyzer.sent" />
                <action android:name="in.wptrafficanalyzer.delivered" />
            </intent-filter>
        </receiver>

        <activity android:name="com.islam.islam.etbara3.FavourityActivity" />
        <activity android:name="com.islam.islam.etbara3.SettingActivity"></activity>
    </application>

</manifest>